RewriteEngine on
RewriteCond %{HTTP_HOST} ^granyupon.com$ [NC,OR]
RewriteCond %{HTTP_HOST} ^www.granyupon.com$
RewriteCond %{REQUEST_URI} !public/
RewriteRule (.*) /public/$1 [L]

# DO NOT REMOVE THIS LINE AND THE LINES BELLOW UPGRADE_INSECURE_REQUESTS:granyupon.com
Header always set Content-Security-Policy: upgrade-insecure-requests
# DO NOT REMOVE THIS LINE AND THE LINES BELLOW UPGRADE_INSECURE_REQUESTS:granyupon.com







# DO NOT REMOVE THIS LINE AND THE LINES BELLOW SSL_REDIRECT:granyupon.com
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} (www\.)?granyupon.com
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
# DO NOT REMOVE THIS LINE AND THE LINES BELLOW SSL_REDIRECT:granyupon.com
